#include <stdio.h>

int main()
{
    int n;
    scanf("%d",&n);
    int a[n][n];
    int i,j,s=0;
    for(i=0;i<n;i++)
    {
        for(j=0;j<n;j++)
        scanf("%d",&a[i][j]);
    }
    for(i=0;i<n;i++)
    {
        for(j=0;j<n;j++)
        if(i==j)
        {
        s=s+a[i][j];    
        }
    }
    printf("%d ",s);

}
Output
2
43
24
25
12
55 

=== Code Execution Successful ===
