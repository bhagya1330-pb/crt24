#include <stdio.h>

int main() 
{
    int n;
    scanf("%d",&n);
    int a[n],i;
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    int large=a[0];
    for(i=1;i<n;i++)
    {
        if(large<a[i])
         large=a[i];
    }
    printf("%d",large);
}  
Output
5
16
18
13
21
14
21

=== Code Execution Successful ===
